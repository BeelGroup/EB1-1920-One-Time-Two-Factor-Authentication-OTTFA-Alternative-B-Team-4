CREATE TABLE user (
	id INTEGER NOT NULL, 
	username VARCHAR(64), 
	user_hash VARCHAR(128), 
	email VARCHAR(120), 
	phone VARCHAR(64), 
	password_hash VARCHAR(128), 
	short_pass1 VARCHAR(64), 
	short_hash1 VARCHAR(128), 
	short_pass2 VARCHAR(64), 
	short_hash2 VARCHAR(128), 
	short_pass3 VARCHAR(64), 
	short_hash3 VARCHAR(128), 
	PRIMARY KEY (id)
);
